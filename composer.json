    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "drupal/core": "dist"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "web/libraries/swordappv2-php-library/swordappclient.php",
            "web/libraries/swordappv2-php-library/packager_mets_swap.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::placeProfile",
            "DrupalProject\\composer\\ScriptHandler::installFullcalendarLibrary",
            "command -v npm install || echo \"NPM not installed. Please install it and run composer install again.\"",
            "DrupalProject\\composer\\ScriptHandler::installBootstrapLibrary",
            "DrupalProject\\composer\\ScriptHandler::installOsToolbarLibrary",
            "./scripts/register-cs.sh",
            "./scripts/install-hooks.sh"
        ],
        "post-update-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::placeProfile",
            "DrupalProject\\composer\\ScriptHandler::installFullcalendarLibrary",
            "command -v npm update || echo \"NPM not installed. Please install it and run composer install.\"",
            "DrupalProject\\composer\\ScriptHandler::installBootstrapLibrary",
            "DrupalProject\\composer\\ScriptHandler::installOsToolbarLibrary"
        ],
        "code-standard": [
            "./vendor/bin/phpcs --standard=Drupal --ignore=*.yml,*.md",
            "./vendor/bin/phpcs --standard=DrupalPractice"
        ],
        "code-standard-fix": [
            "./vendor/bin/phpcbf --standard=Drupal"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/libraries/chosen": [
                "npm-asset/chosen-js"
            ],
            "web/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "web/libraries/blazy": [
                "npm-asset/blazy"
            ],
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "source": [
                "https://raw.githubusercontent.com/drupal/drupal/{version}/{path}"
            ]
        },
        "patches": {
