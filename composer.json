            },
            "drupal/courier": {
                "Fixes checkboxes count warning": "https://github.com/dpi/courier/pull/48.patch"
            },
            "drupal/group": {
                "/views/ajax?_wrapper_format=drupal_ajax HTTP/1.1 - HTTP/1.1 403 Forbidden": "https://www.drupal.org/files/issues/2018-08-15/group-route-match-ajax-views-d8-2942657-11.patch",
                "Integrity constraint violation: Duplicate entry for key PRIMARY in node_access": "https://www.drupal.org/files/issues/2019-12-30/2892512-31.patch",
                "New group content created when node edited": "https://www.drupal.org/files/issues/2019-06-04/3059300-3.patch",
                "Prevent resaving of deleted entities": "https://raw.githubusercontent.com/openscholar/openscholar/ae879229e866cbc96d91d2b2a8273292fa26fd34/patches/group.fix_saving_deleted_entity.patch",
                "Entities identified by strings as group content": "https://www.drupal.org/files/issues/2019-12-23/group-support_string_entity_ids-2797793-188.patch",
                "Group creator must complete their membership Wizard duplicates membership": "https://www.drupal.org/files/issues/2019-07-09/group_membership_duplication-3016197-10.patch"
            },
            "drupal/shorten": {
                "Update is.gd service api url": "https://www.drupal.org/files/issues/2019-07-03/update-isgd-api-3065567-2.patch"
            },
            "drupal/bibcite": {
                "Extending the supported bibcite formats.": "patches/bibcite_formats_extension.patch",
                "Editors and others roles treated as author": "https://www.drupal.org/files/issues/2019-08-01/other-roles-shown-as-authors-3006180-6.patch"
            },
            "drupal/config_ignore": {
                "Override drush cim/cex for filtering": "https://www.drupal.org/files/issues/2019-01-08/2973431-supplementary-drush-commands-17.patch",
                "Allow adding continer filtering": "https://www.drupal.org/files/issues/2019-12-27/config-ignore-collections-2973431-26.patch"
            },
            "drupal/disqus": {
                "Disqus migrate support is broken": "https://www.drupal.org/files/issues/disqus-migrate-support-2879592-14.patch"
            },
            "drupal/migrate_tools": {
                "Warning: session_id(): Cannot change session id when headers already sent": "https://www.drupal.org/files/issues/2019-03-09/3038712-2.patch"
            },
            "drupal/node_revision_delete": {
